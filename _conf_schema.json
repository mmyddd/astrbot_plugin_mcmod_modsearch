{
    "max_single_results": {
        "description": "单类型搜索最大显示结果数",
        "type": "int",
        "hint": "请输入1-50之间的数字",
        "default": 20
    },
    "max_multi_results": {
        "description": "全搜索中各类型最大显示结果数",
        "type": "int",
        "hint": "请输入1-20之间的数字",
        "default": 10
    },
    "api_timeout": {
        "description": "API请求超时时间(秒)",
        "type": "int",
        "hint": "请输入5-30之间的数字",
        "default": 15
    },
    "enable_all_search": {
        "description": "是否启用全搜索功能",
        "type": "bool",
        "hint": "true或false",
        "default": true
    }
}
